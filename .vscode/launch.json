  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python Debugger: Current File",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "args": "rick",
      "justMyCode": false
    },
    {
      "name": "Python Debugger: Behave",
      "type": "debugpy",
      "request": "launch",
      "module": "behave",
      "console": "integratedTerminal",
      "args": "-t wip -k",
      "cwd": "${workspaceFolder}/bdd",
      "justMyCode": false
    }
  ]
}
